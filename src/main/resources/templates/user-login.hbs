{{#partial "content"}}
    <h1>User Login</h1>

<div class="row">

        <div class="yellow">
    <form action="/users/login" method="post">


        <label for="inputUserLogin">Email</label>
        <input id="inputUserLogin" name="inputUserLogin" type="email" class="form-control">

        <label for="inputUserPassword">Password</label>
        <input id="inputUserPassword" name="inputUserPassword" type="password" class="form-control">

        <button type="submit" class="btn">Submit</button>


    </form>
        </div>
    </div>

{{/partial}}

