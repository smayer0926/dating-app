{{#partial "content"}}


    {{#if users}}
        {{#each users }}
            <div class="emphasized well">
                <h3>User Information</h3>
                <label for="name">User name:</label>
                <input id ="name" name="name" type="text" class="form-control" value="{{name}}">
                <!--<p>User name: {{name}}</p>-->
                <!--<p>age: {{age}}</p>-->
                <label for="age">User Age:</label>
                <input id ="age" name="age" type="text" class="form-control" value="{{age}}">
                <!--<p>gender: {{gender}}</p>-->
                <label for="gender">User Age:</label>
                <input id ="gender" name="gender" type="text" class="form-control" value="{{gender}}">
                <!--<p>gender preference: {{genderPreference}}</p>-->
                <label for="genderPreference">User Age:</label>
                <input id ="genderPreference" name="genderPreference" type="text" class="form-control" value="{{genderPreference}}">
                <!--<p>preferred minimum age: {{matchMinAge}}</p>-->
                <label for="matchMinAge">User Minimum Age:</label>
                <input id ="matchMinAge" name="matchMinAge" type="text" class="form-control" value="{{matchMinAge}}">
                <!--<p>preferred maximum age: {{matchMaxAge}}</p>-->
                <label for="matchMaxAge">User Maximum Age:</label>
                <input id ="matchMaxAge" name="matchMaxAge" type="text" class="form-control" value="{{matchMaxAge}}">
                <!--<p>area: {{zip}}</p>-->
                <label for="zip">Zip Code: </label>
                <input id ="zip" name="zip" type="text" class="form-control" value="{{zip}}">
                <!--<p>email: {{email}}</p>-->
                <label for="email">Email: </label>
                <input id ="email" name="email" type="text" class="form-control" value="{{email}}">
                <div class="col-md-6">
                    <label for="photo">Add an Image</label>
                    <img src="" height = "200">
                    <input id="photo" name="photo" type="image" class = "form-control">
                </div>
            </div>
        {{/each}}
    {{else}}
        <p>Please add a user!</p>
    {{/if}}

{{/partial}}

{{> layout.hbs}}